{
  "name": "@dk/svelte",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --mode standalone",
    "build": "vite build --no-watch",
    "build-sa": "vite build --mode standalone --watch",
    "preview": "vite preview",
    "start": "pnpm run build && concurrently \"pnpm run preview\" \"pnpm run build-sa\"",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier --list-different --write ."
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/svelte": "^4.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@tsconfig/svelte": "^5.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/svelte-range-slider-pips": "^2.0.4",
    "@vitest/coverage-v8": "^1.3.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "resize-observer-polyfill": "^1.5.1",
    "rollup-plugin-livereload": "^2.0.5",
    "single-spa-svelte": "^2.1.1",
    "svelte": "^4.2.10",
    "svelte-check": "^3.6.3",
    "svelte-jester": "^3.0.0",
    "svelte-range-slider-pips": "^2.3.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vite-plugin-single-spa": "^0.6.0",
    "vitest": "^1.2.2",
    "vitest-canvas-mock": "^0.3.3"
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.0",
    "chart.js": "^4.4.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-zoom": "^2.0.1",
    "date-fns": "^3.3.1"
  }
}
