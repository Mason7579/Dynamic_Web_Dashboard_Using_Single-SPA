{
  "name": "rig-overview-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build --no-watch ",
    "preview": "vite preview",
    "build-sa": "vite build --mode standalone --watch",
    "start": "pnpm run build && concurrently \"pnpm run preview\" \"pnpm run build-sa\"",
    "test": "vitest --coverage --no-watch",
    "lint": "eslint .",
    "format": "prettier --list-different --write ."
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.0",
    "vue": "^3.4.19"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/vue": "^8.0.2",
    "@types/node": "^20.11.20",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.3.1",
    "@vue/compiler-dom": "^3.4.19",
    "@vue/server-renderer": "^3.4.19",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "single-spa-vue": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-single-spa": "^0.6.0",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27"
  }
}
